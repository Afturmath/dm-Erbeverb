@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<urn:maxgen:dm-Erbeverb>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-erbeverb.html> ;
        modgui:stylesheet <modgui/stylesheet-erbeverb.css> ;
        modgui:screenshot <modgui/dm-Erbeverb-screenshot.png> ;
        modgui:thumbnail <modgui/dm-Erbeverb-thumbnail.png> ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "reverse" ;
            lv2:name "reverse" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "size" ;
            lv2:name "size" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "speed" ;
            lv2:name "speed" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "depth" ;
            lv2:name "depth" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "predelay" ;
            lv2:name "predelay" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "tilt" ;
            lv2:name "tilt" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "absorb" ;
            lv2:name "absorb" ;
        ] , [
            lv2:index 7 ;
            lv2:symbol "decay" ;
            lv2:name "decay" ;
        ] , [
            lv2:index 8 ;
            lv2:symbol "mix" ;
            lv2:name "mix" ;
        ] ;
    ] .